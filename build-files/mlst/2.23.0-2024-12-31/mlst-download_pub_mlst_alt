#!/usr/bin/env perl
use strict;
use Getopt::Std;
use File::Basename;
use File::Fetch;
use Data::Dumper;
use File::Which;
use Cwd 'abs_path';

my $EXE = basename($0);
my $URL = 'https://pubmlst.org/data/dbases.xml';
my $CURL = "curl --silent";

my %opt = (
  'd' => '',
  'x' => 'afumigatus,blastocystis,calbicans,cglabrata,ckrusei,ctropicalis,csinensis,kseptempunctata,sparasitica,tvaginalis',
  'v' => 0,
  'j' => 1,
  'n' => 0,
);

sub msg { print STDERR "@_\n"; }
sub wrn { msg("WARNING:", @_); }
sub err { msg("ERROR:", @_); exit(-1); }

sub usage {
  print<<"EOHELP";
SYNOPSIS
  Download PubMLST schemes
USAGE
  $EXE [options] -d OUTDIR
OPTIONS
  -h       Show this help
  -v       Verbose output
  -j CPUS  Parallel download threads [$opt{j}]
  -x CSV   Ignore these schemes [$opt{x}]
  -d DIR   Output folder [$opt{d}]
  -n       Don't run download commands
END
EOHELP
  exit($_[0]);
}

getopts('hx:d:j:vn', \%opt) or usage(1);
$opt{'h'} and usage(0);
my $dir = $opt{'d'} or err("Need -d output folder");
$dir = abs_path($dir);
my %ignore = (map { $_=>1 } split m/,/, $opt{x});
my $verbose = $opt{'v'};
my $cpus = $opt{'j'};
$cpus && $cpus =~ m/^\d+$/ or err("-j must be a integer >= 1");

msg("This is $EXE");

for my $exe ('curl', 'parallel') {
  my $path = which($exe);
  $path ? msg("Found $exe - $path")
        : err("Please install '$exe'");
}

if (not -d $dir) {
  msg("Making dir: $dir");
  mkdir $dir or err("Could not make dir"); 
}
else {
  msg("Overwriting exsitng dir: $dir");
}

# Use local file instead of downloading
my $fn = 'dbases.xml';
msg("Using local file:", $fn);

open my $fh, '<', $fn or err("Could not open file: $fn");
my $xml = join '', <$fh>;
close $fh;
$xml =~ s/[\n\r]//g;
msg("Loaded XML", length($xml), "bytes");

# msg("Downloading:", $URL);
# my $ff = File::Fetch->new(uri=>$URL);
# #msg(Dumper($ff));
# my $fn = "$dir/".$ff->file;
# if (-r $fn) {
#   msg("Re-using existing: $fn");
# } else {
#   $fn = $ff->fetch(to=>$dir) or err($ff->error);
# }
# msg("Got:", $fn);

# open my $fh, '<', $fn;
# my $xml = join '', <$fh>;
# close $fh;
# $xml =~ s/[\n\r]//g;
# msg("Loaded XML", length($xml), "bytes");
#msg($xml);

# https://rest.pubmlst.org/db/pubmlst_achromobacter_seqdef/schemes/1/profiles_csv
# https://rest.pubmlst.org/db/pubmlst_achromobacter_seqdef/loci/nusA/alleles_fasta
my $S=0;
my %get;
my $scheme;
while ($xml =~ m'<url>(.*?)</url>'gi) {
  my $url = $1;
  msg("URL:", $1) if $verbose;
  if ($url =~ m'pubmlst_(\w+?)_seqdef/schemes/(\d+)/') {
    msg(++$S, "scheme:", $1, "version", $2);
    $scheme = $2==1 ? $1 : "${1}_${2}";
    $scheme or err("Empty scheme!");
    $get{$scheme}{profile} = $url;
  }
  elsif ($url =~ m'pubmlst_(\w+?)_seqdef/loci/(\w+?)/alleles') {
    $get{$scheme}{gene}{$2} = $url;
    msg("  => $scheme allele $2") if $verbose;
    $scheme =~ m/$1/ or err("Scheme order problem: $scheme/$1");
  }
}

$S or err("No schemes found");

msg("Deleting schemes specified by -x");
for my $s (sort keys %ignore) {
  if (exists $get{$s}) {
    delete $get{$s};
    msg("Ignoring: $s");
    $S--;
  }
  else {
    wrn("Can't ignore non-existent scheme: $s");
  }
}

msg("Collated $S schemes");
print STDERR Dumper(\%get) if $verbose;

my $dl_fn = "$dir/dbases.sh";
msg("Writing download commands: $dl_fn");
open my $dl, '>', $dl_fn;
for my $s (sort keys %get) {
  print $dl "mkdir -p '$dir/$s'\n";
  print $dl "$CURL --output '$dir/$s/$s.txt' '$get{$s}{profile}'\n";
  for my $g (keys %{$get{$s}{gene}}) {
    print $dl "$CURL --output '$dir/$s/$g.tfa' '$get{$s}{gene}{$g}'\n";
  }
}
close $dl;

if ($opt{n}) {
  msg("Download commands in: $dl_fn");
}
else {
  msg("Downloading $cpus files at a time...");
  my $cmd = "parallel -k -v -a '$dl_fn' -j $cpus";
  system($cmd)==0 or err("Download didn't fully complete!");
}

msg("Done.");

__DATA__

